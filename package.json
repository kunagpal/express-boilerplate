{
  "name": "express-boilerplate",
  "description": "A project scaffold for use with Express.js",
  "keywords": [
    "express",
    "boilerplate",
    "skeleton"
  ],
  "homepage": "https://github.com/kunagpal/express-boilerplate",
  "bugs": {
    "url": "https://waffle.io/kunagpal/express-boilerplate"
  },
  "version": "0.0.10",
  "private": true,
  "scripts": {
    "archive": "node scripts/package/archive.js",
    "check": "node node_modules/.bin/npm-check -u",
    "make-wiki": "node scripts/docs/make-wiki.js",
    "make-docs": "node node_modules/.bin/jsdoc -c .jsdocrc bin database routes scripts utils app.js",
    "start": "node scripts/app/start.js",
    "stop": "node scripts/app/stop.js",
    "seed": "node scripts/database/seed.js",
    "purge": "node scripts/database/purge.js",
    "security": "node scripts/test/security.js",
    "csslint": "node node_modules/csslint/dist/cli.js public/stylesheets",
    "eslint": "node scripts/test/esLint.js",
    "lint": "npm run eslint && npm run csslint",
    "unit": "node scripts/misc/test.js",
    "e2e": "node scripts/test/e2e.js",
    "structure": "node scripts/test/structure.js",
    "test": "npm run csslint && node scripts/test",
    "prestart": "node scripts/misc/prestart.js",
    "postinstall": "node scripts/misc/postInstall.js"
  },
  "author": "Kunal Nagpal <kunagpal@gmail.com>",
  "dependencies": {
    "async": "2.3.0",
    "body-parser": "1.17.1",
    "bower": "1.8.0",
    "clean-css": "4.0.12",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.3",
    "csurf": "1.9.0",
    "debug": "2.6.3",
    "ejs": "2.5.6",
    "emailjs": "1.0.8",
    "express": "5.0.0-alpha.5",
    "express-session": "1.15.2",
    "express-status-monitor": "0.1.9",
    "helmet": "3.5.0",
    "lodash": "4.17.4",
    "mongodb": "2.2.25",
    "morgan": "1.8.1",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth2": "0.1.6",
    "pm2": "2.4.4",
    "raven": "1.2.1",
    "require-all": "2.1.0",
    "uglify-js": "2.8.22"
  },
  "devDependencies": {
    "chalk": "1.1.3",
    "csslint": "1.0.5",
    "dotenv": "4.0.0",
    "editorconfig": "0.13.2",
    "eslint": "3.19.0",
    "eslint-plugin-jsdoc": "3.0.1",
    "eslint-plugin-lodash": "2.4.0",
    "eslint-plugin-mocha": "4.9.0",
    "eslint-plugin-mongodb": "0.2.4",
    "eslint-plugin-security": "1.3.0",
    "faker": "4.1.0",
    "figlet": "1.2.0",
    "ink-docstrap": "1.3.0",
    "istanbul": "0.4.5",
    "js-yaml": "3.8.3",
    "jsdoc": "3.4.3",
    "jsdoc-to-markdown": "3.0.0",
    "mocha": "3.2.0",
    "npm-check": "5.4.0",
    "nsp": "2.6.3",
    "parse-gitignore": "0.4.0",
    "recursive-readdir": "2.1.1",
    "supertest": "3.0.0"
  },
  "optionalDependencies": {
    "bcrypt": "1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunagpal/express-boilerplate"
  },
  "license": "GPL-3.0+",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  }
}
