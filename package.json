{
  "name": "express-boilerplate",
  "description": "A project scaffold for use with Express.js",
  "keywords": [
    "express",
    "boilerplate",
    "skeleton"
  ],
  "homepage": "https://github.com/kunagpal/express-boilerplate",
  "bugs": {
    "url": "https://waffle.io/kunagpal/express-boilerplate"
  },
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "node node_modules/.bin/npm-check -u",
    "docs": "node node_modules/.bin/jsdoc -c .jsdocrc bin routes scripts utils app.js",
    "start": "node scripts/app/start.js",
    "restart": "node scripts/app/restart.js",
    "stop": "node scripts/app/stop.js",
    "seed": "node scripts/database/seed.js",
    "purge": "node scripts/database/purge.js",
    "security": "node scripts/test/security.js",
    "csslint": "node node_modules/csslint/dist/cli.js public/stylesheets",
    "eslint": "node scripts/test/esLint.js",
    "lint": "npm run eslint && npm run csslint",
    "unit": "node scripts/misc/test.js",
    "e2e": "node scripts/test/e2e.js",
    "structure": "node scripts/test/structure.js",
    "test": "npm run csslint && node scripts/test",
    "pack": "node scripts/misc/pack.js",
    "postinstall": "node scripts/misc/postInstall.js"
  },
  "contributors": [
    {
      "name": "Kunal Nagpal",
      "email": "kunagpal@gmail.com"
    }
  ],
  "dependencies": {
    "async": "2.1.5",
    "body-parser": "1.16.1",
    "bower": "1.8.0",
    "clean-css": "4.0.8",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "csurf": "1.9.0",
    "debug": "2.6.1",
    "ejs": "2.5.6",
    "emailjs": "1.0.8",
    "express": "4.14.1",
    "express-session": "1.15.1",
    "express-status-monitor": "0.1.9",
    "helmet": "3.4.1",
    "heroku-client": "3.0.2",
    "lodash": "4.17.4",
    "mongodb": "2.2.24",
    "morgan": "1.8.1",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth2": "0.1.6",
    "pm2": "2.4.0",
    "raven": "1.1.3",
    "uglify-js": "2.8.0"
  },
  "devDependencies": {
    "chalk": "1.1.3",
    "csslint": "1.0.5",
    "dotenv": "4.0.0",
    "editorconfig": "0.13.2",
    "eslint": "3.16.1",
    "eslint-plugin-jsdoc": "2.4.0",
    "eslint-plugin-lodash": "2.3.5",
    "eslint-plugin-mocha": "4.8.0",
    "eslint-plugin-mongodb": "0.2.4",
    "eslint-plugin-security": "1.3.0",
    "faker": "4.1.0",
    "figlet": "1.2.0",
    "ink-docstrap": "1.3.0",
    "istanbul": "0.4.5",
    "js-yaml": "3.8.1",
    "jsdoc": "3.4.3",
    "jsdoc-to-markdown": "3.0.0",
    "mocha": "3.2.0",
    "npm-check": "5.4.0",
    "nsp": "2.6.2",
    "parse-gitignore": "0.3.1",
    "recursive-readdir": "2.1.1",
    "supertest": "3.0.0"
  },
  "optionalDependencies": {
    "bcrypt": "1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunagpal/express-boilerplate"
  },
  "license": "GPL-3.0+",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  }
}
